<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/main.css" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css"
      integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Jost:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Montserrat:ital,wght@0,400;1,200&family=Mulish:ital,wght@0,200;0,300;0,400;0,500;0,600;0,700;0,800;1,200;1,300&family=Nunito+Sans:ital,wght@0,200;0,300;0,400;0,600;0,700;1,200;1,300;1,400;1,600;1,700;1,800&family=Smooch&display=swap"
      rel="stylesheet"
    />
    <title>furniture contact</title>
  </head>
</html>
<body>
  <%-include("./inc/header")%>
  <!-- product  -->
  <section class="product">
    <h1 class="product__title"><%=data.title%></h1>
    <div class="product__slider">
      <div class="product__slides">
        <%data.images.forEach(image=>{%>
        <div class="product__slide">
          <img
            class="product__slide--img"
            src="/img/<%=image.filename%>"
            alt=""
          />
        </div>
        <%})%>
      </div>
    </div>
    <div class="product__angle">
      <button class="product__angle--left">
        <i class="fas fa-angle-left"></i>
      </button>
      <button class="product__angle--right">
        <i class="fas fa-angle-right"></i>
      </button>
    </div>
    <div class="product__info">
      <pre class="product__info--desc"><%=data.desc%></pre>
    </div>
  </section>
  <!-- instagram -->
  <!-- footer  -->
  <%- include("./inc/footer")%>

  <script type="module" src="/product.js"></script>
</body>
